{
  "name": "semrush-email-scraper",
  "version": "1.0.0",
  "description": "Outil de scraping SEMrush et d'extraction d'emails",
  "main": "dev.js",
  "scripts": {
    "dev": "node dev.js",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "migrate": "cd backend && npm run migrate",
    "seed": "cd backend && npm run seed",
    "start:backend": "cd backend && npm run start",
    "start:frontend": "cd frontend && npm run start",
    "build:frontend": "cd frontend && npm run build",
    "generate-sample": "cd examples && node generate_sample_output.js",
    "test": "node test-suite.js"
  },
  "keywords": [
    "semrush",
    "scraper",
    "email",
    "puppeteer",
    "nextjs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "concurrently": "^8.2.2",
    "xlsx": "^0.18.5"
  }
}